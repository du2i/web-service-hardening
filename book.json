{
    "title": "견고한 웹 서비스를 만들기 위한 실용적인 보안 가이드",
    "styles": {
        "website": "styles/website.css",
        "ebook": "styles/ebook.css",
        "pdf": "styles/pdf.css",
        "mobi": "styles/mobi.css",
        "epub": "styles/epub.css"
    },
    "plugins": [
        "etoc",
        "flexible-alerts",
        "disqus",
        "ga"
    ],
    "pluginsConfig": {
        "etoc": {
            "h2lb": 3,
            "mindepth": 3,
            "maxdepth": 4,
            "notoc": false
        },
        "disqus": {
            "shortName": "lesstif-gitbook-io"
        },
        "ga": {
            "token": "UA-135306060-1"
        },
        "flexible-alerts": {
            "style": "flat"
        }
    }
}
